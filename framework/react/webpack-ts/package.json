{
  "name": "@wx/webpack-react-ts",
  "libraryName": "wx",
  "libraryEntry": "index.umd.js",
  "version": "0.0.1",
  "description": "Boilerplate for webpack & react, with typeScript",
  "main": "build/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/fe-boilerplate"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wxyyxc1992@github",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dashboard -- npm run dev:node",
    "dev": "parallelshell \"npm run mock\" \"npm run dev\"",
    "dev:node": "webpack-dev-server --config ./dev-config/webpack.config.dev.js --hot",
    "build": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.prod.js --mode production",
    "build:dev": "npm run clean && webpack --config ./dev-config/webpack.config.prod.js --mode development",
    "build:lib": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.umd.js --mode production",
    "stats": "webpack --config ./dev-config/webpack.config.prod.js --mode production --profile --json > build/stats.json",
    "analyze": "npm run stats && webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf build && mkdir build",
    "test": "jest",
    "update": "npm-check -u",
    "lint": "tslint -c tslint.json 'src/**/*.(ts|tsx)'",
    "mock": "json-server --watch dev-config/mock/db.json --port 9091",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "prettier-all": "prettier --write 'src/**/*' '!src/{assets,datas}/**'"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/dev-config/mock/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/dev-config/mock/fileMock.js"
    },
    "testMatch": [
      "**/*.test.(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "antd": "^3.11.6",
    "class-names": "^1.0.0",
    "mobx": "5.8.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.4",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.4",
    "@storybook/addon-links": "^4.1.4",
    "@storybook/addons": "^4.1.4",
    "@storybook/react": "^4.1.4",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "23.3.11",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.12",
    "@types/redux": "^3.6.0",
    "autoprefixer": "9.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "enzyme": "^3.8.0",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "json-server": "0.14.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mobx-react-devtools": "6.0.3",
    "node-sass": "^4.11.0",
    "npm-check": "^5.9.0",
    "offline-plugin": "^5.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.6.3",
    "resolve-url-loader": "3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "ts-loader": "5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.28.3",
    "webpack-bundle-analyzer": "3.0.3",
    "webpack-cli": "3.2.0",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "^3.1.14",
    "workerize-loader": "^1.0.4"
  }
}
