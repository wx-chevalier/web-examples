{
  "name": "@wx/webpack-react",
  "libraryName": "wx",
  "libraryEntry": "index.umd.js",
  "version": "0.0.1",
  "description": "Boilerplate for webpack & react",
  "main": "build/index.umd.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/fe-boilerplate"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wxyyxc1992@github",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss)$": "<rootDir>/dev-config/mock/styleMock.js",
      "\\.(gif|ttf|eot|svg)$": "<rootDir>/dev-config/mock/fileMock.js"
    }
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "start": "webpack-dashboard -- npm run dev:node",
    "dev": "parallelshell \"npm run mock\" \"npm start\"",
    "dev:node": "webpack-dev-server --config ./dev-config/webpack.config.dev.js --hot",
    "build": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.prod.js --mode production",
    "build:dev": "npm run clean && webpack --config ./dev-config/webpack.config.prod.js --mode development",
    "build:lib": "npm run clean && BABEL_ENV=production webpack --config ./dev-config/webpack.config.umd.js --mode production",
    "stats": "webpack --config ./dev-config/webpack.config.prod.js --mode production --profile --json > build/stats.json",
    "analyze": "npm run stats && webpack-bundle-analyzer build/stats.json",
    "clean": "rimraf build && mkdir build",
    "test": "jest",
    "update": "npm-check -u",
    "lint": "eslint --ext .js,.jsx src test",
    "mock": "json-server --watch dev-config/mock/db.json --port 9091",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "precommit": "lint-staged",
    "prettier-all": "prettier --write 'src/**/*' '!src/{assets,datas}/**'"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "antd": "^3.14.1",
    "babel-polyfill": "^6.26.0",
    "class-names": "^1.0.0",
    "mobx": "5.9.0",
    "mobx-react": "^5.4.3",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "4.0.1",
    "redux-actions": "^2.6.5",
    "redux-promise": "^0.6.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-proposal-decorators": "^7.3.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "@storybook/addon-actions": "^5.0.0",
    "@storybook/addon-links": "^5.0.0",
    "@storybook/addons": "^5.0.0",
    "@storybook/react": "^5.0.0",
    "autoprefixer": "9.4.10",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "^24.3.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "copy-webpack-plugin": "^5.0.0",
    "css-loader": "2.1.1",
    "enzyme": "^3.9.0",
    "eslint": "5.15.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "24.3.1",
    "jest-cli": "24.3.1",
    "json-server": "0.14.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "npm-check": "^5.9.0",
    "offline-plugin": "5.0.6",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "3.0.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.8.0",
    "regenerator-runtime": "0.13.1",
    "resolve-url-loader": "^3.0.1",
    "rimraf": "^2.6.3",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "^3.2.1",
    "workerize-loader": "^1.0.4"
  }
}
