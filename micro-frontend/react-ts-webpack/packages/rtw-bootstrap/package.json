{
  "name": "@wx/rtw-bootstrap",
  "version": "0.0.1",
  "description": "@wx/rtw-bootstrap",
  "repository": {
    "type": "git",
    "url": "https://github.com/wxyyxc1992/fe-boilerplate"
  },
  "license": "MIT",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "wxyyxc1992@github",
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "lint-staged": {
    "*.{ts,tsx,scss,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "analyze": "npm run stats && webpack-bundle-analyzer dist/stats.json",
    "clean": "rimraf dist",
    "dev": "webpack-dev-server --config ./scripts/webpack/webpack.config.dev.js --hot",
    "build": "npm run build:prod",
    "build:prod": "NODE_ENV=production webpack --config ./scripts/webpack/webpack.config.prod.js",
    "start": "webpack-dashboard -- npm run dev",
    "stats": "webpack --config ./scripts/webpack.config.prod.js --mode production --profile --json > dist/stats.json"
  },
  "dependencies": {
    "@wx/rtw-core": "0.0.1",
    "@wx/rtw-host-app": "0.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "@types/jest": "23.3.11",
    "@types/react-dom": "^16.0.11",
    "autoprefixer": "9.4.4",
    "awesome-typescript-loader": "^5.2.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "2.1.0",
    "enzyme": "^3.8.0",
    "file-loader": "3.0.1",
    "fork-ts-checker-webpack-plugin": "^0.5.2",
    "html-webpack-plugin": "^3.2.0",
    "html-webpack-template": "^6.2.0",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "json-server": "0.14.2",
    "lerna": "^3.0.0-rc.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "parallelshell": "^3.0.2",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "^4.6.3",
    "resolve-url-loader": "3.0.0",
    "rimraf": "^2.6.3",
    "style-loader": "0.23.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "5.3.2",
    "tslint": "^5.12.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0",
    "typescript": "3.2.2",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "^1.1.2",
    "wasm-loader": "^1.3.0",
    "webpack": "^4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dashboard": "2.0.0",
    "webpack-dev-server": "^3.1.14",
    "workerize-loader": "^1.0.4"
  },
  "files": [
    "dist/"
  ]
}
